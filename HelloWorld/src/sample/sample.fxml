<?import javafx.scene.control.TableColumn?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <columns>
            <TableColumn fx:id="sid" text="SID"></TableColumn>
            <TableColumn fx:id="assig" text="Assignment"></TableColumn>
            <TableColumn fx:id="midt" text="midt"></TableColumn>
            <TableColumn fx:id="finale" text="finale"></TableColumn>
            <TableColumn fx:id="finalm" text="finalm"></TableColumn>
            <TableColumn fx:id="grade" text="grade"></TableColumn>
        </columns>
    </TableView>
</GridPane>